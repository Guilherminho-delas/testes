#include <stdio.h>

int main()
{
//variaveis
int num;
int binario;
int bit1, bit2, bit3, bit4, bit5;
int cont; cont = 0;
//inicio
scanf ("%d", &num);

bit1 = num%2;
num /= 2;

bit2 = num%2;
num /= 2;

bit3 = num%2;
num /= 2;

bit4 = num%2;

bit5 = num/2;

if (bit1 == 1)
{
    cont++;
}

if (bit2 == 1)
{
    cont++;
}

if (bit3 == 1)
{
    cont++;
}

if (bit4 == 1)
{
    cont++;
}

if (bit5 == 1)
{
    cont++;
}

printf ("O binario tem %d 1", cont);

}
